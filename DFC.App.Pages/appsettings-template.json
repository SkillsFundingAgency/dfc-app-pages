{
  "APPINSIGHTS_INSTRUMENTATIONKEY": "<<Key Here>>",
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Microsoft": "Trace"
      }
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "ApplicationName": "DFC-App-Pages"
  },
  "Policies": {
    "HttpCircuitBreaker": {
      "DurationOfBreak": "00:01:00",
      "ExceptionsAllowedBeforeBreaking": 3
    },
    "HttpRetry": {
      "BackoffPower": 2,
      "Count": 3
    }
  },
  "Cms": {
    "TokenEndPointUrl": "https://localhost/",
    "GraphApiUrl": "https://localhost/",
    "ClientId": "id",
    "ClientSecret": "secret",
    "RedisCacheConnectionString": "connectionstring",
    "SqlApiUrl": "https://localhost/",
    "Expiry": "4"
  },
  "contentMode": {
    "contentMode": "PUBLISHED"
  }
}